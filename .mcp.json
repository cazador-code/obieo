{
  "mcpServers": {
    "gtm": {
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://gtm-mcp.stape.ai/mcp"]
    },
    "stripe": {
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://mcp.stripe.com"]
    },
    "clerk": {
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://mcp.clerk.com/mcp"]
    },
    "analytics-mcp": {
      "command": "pipx",
      "args": ["run", "analytics-mcp"]
    },
    "airtable": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.airtable.com/mcp",
        "3334",
        "--static-oauth-client-info",
        "{\"client_id\":\"8a135df9-5640-4e7a-a422-21f13edaa347\"}",
        "--debug"
      ]
    }
  }
}
